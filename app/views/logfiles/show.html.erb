<h4> Simulation Time: </h4>
<p><b> <%= @summary.sim_time %> </b></p>

<script>
$(document).ready(function () {
function drawBasic() {
    var variable = [4,5,10,12];

    var data=[];
    var options=[];
    var chart=[];
    for (var ind=0; ind<4; ind++) {
        data[ind] = new google.visualization.DataTable();
        data[ind].addColumn('number', 'Time');
        for (var dev=0; dev < iostat_log.dev.length; dev++)
            data[ind].addColumn('number', iostat_log.dev[dev]);

        for (var t in iostat_log.time) {
            var row = [iostat_log.time[t]];
            for (dev = 0; dev < iostat_log.dev.length; dev++)
                row.push(iostat_log.dat[dev][variable[ind]][t]);
            data[ind].addRow(row);
        }

        options[ind] = {
            hAxis: {
                title: 'Time'
            },
            vAxis: {
                title: iostat_log.var[variable[ind]]
            }
        };
        chart[ind] = new google.visualization.LineChart(document.getElementById('chart'+ind+'_div'));
        chart[ind].draw(data[ind], options[ind]);
    }

}

    google.load('visualization', '1', {packages: ['corechart', 'line'], "callback": drawBasic});

});
</script>

